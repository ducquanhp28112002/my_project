   "source": [
    "import turtle as t\n",
    "\n",
    "a = int(input(\"Nhập vào độ dài cạnh hình vuông: \"))\n",
    "count = 0\n",
    "\n",
    "\n",
    "t.fillcolor(\"red\")\n",
    "t.begin_fill()\n",
    "while count < 4:\n",
    "    t.forward(a)\n",
    "    t.left(90)\n",
    "    count += 1\n",
    "t.end_fill()\n",
    "t.done()"
   ]
  }
