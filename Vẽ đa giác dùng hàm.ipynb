   "source": [
    "import turtle as pen\n",
    "\n",
    "pen.hideturtle()\n",
    "pen.pencolor(\"red\")\n",
    "\n",
    "\n",
    "def draw(n, a):\n",
    "    pen.fillcolor(\"blue\")\n",
    "    pen.begin_fill()\n",
    "    pen.circle(a, steps = n)\n",
    "    pen.end_fill()\n",
    "    pen.done()\n",
    "\n",
    "draw(8, 100)"
   ]
  }
