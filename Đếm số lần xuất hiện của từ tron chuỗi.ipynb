   "source": [
    "string = 'Welcome to my house'\n",
    "str = string.split(\" \")\n",
    " \n",
    "count = {}\n",
    "for i in str:\n",
    "    if i in count:\n",
    "        count[i]+=1\n",
    "    else:\n",
    "        count[i]=1\n",
    "\n",
    "for i in sorted(count, key=count.get, reverse=False):\n",
    "    print(i, count[i], end = \", \")"
   ]
