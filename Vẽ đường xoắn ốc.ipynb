    "import turtle as pen\n",
    "\n",
    "r = 10\n",
    "\n",
    "pen.left(90)\n",
    "while True:\n",
    "    pen.circle(r, 180)\n",
    "    r += 5\n",
    "    if pen.distance((0,0)) > 100:\n",
    "        break\n",
    "pen.done()"
   ]
